<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="Background" prefHeight="200.0" prefWidth="200.0" stylesheets="@ScheduleStyle.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Pane id="background" layoutX="-242.0" prefHeight="496.0" prefWidth="1013.0">
         <children>
            <Label id="ScheduleLabel" fx:id="ScheduleLabel" layoutX="284.0" layoutY="6.0" prefHeight="17.0" prefWidth="184.0" stylesheets="@ScheduleStyle.css" text="Schedule">
               <font>
                  <Font size="37.0" />
               </font>
            </Label>
            <Label layoutX="284.0" layoutY="64.0" prefHeight="17.0" prefWidth="49.0" text="Week of:">
               <font>
                  <Font size="11.0" />
               </font>
            </Label>
            <TableView fx:id="table" layoutX="283.0" layoutY="96.0" prefHeight="368.0" prefWidth="679.0">
              <columns>
                <TableColumn id="employeeColumn" fx:id="name" prefWidth="75.0" text="Employee" />
                <TableColumn id="day1Column" fx:id="day1Column" prefWidth="75.0" text="Monday" />
                  <TableColumn id="day2Column" fx:id="day2Column" prefWidth="75.0" text="Tuesday" />
                  <TableColumn id="day3Column" fx:id="day3Column" prefWidth="75.0" text="Wednesday" />
                  <TableColumn id="day4Column" fx:id="day4Column" prefWidth="75.0" text="Thursday" />
