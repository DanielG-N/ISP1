<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="749.0" prefWidth="1466.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Inventory.HelloApplication">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Pane prefHeight="699.0" prefWidth="829.0">
        <children>
            <ButtonBar prefHeight="40.0" prefWidth="200.0" />
            <Label layoutX="12.0" prefHeight="91.0" prefWidth="968.0" style="-fx-background-color: beige;" text="Inventory ">
                <padding>
                    <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
                </padding>
                <font>
                    <Font name="Bookshelf Symbol 7" size="55.0" />
                </font>
            </Label>
            <Button id="addButton" fx:id="btnAdd" layoutX="739.0" layoutY="579.0" mnemonicParsing="false" onAction="#btnAddItem" style="-fx-background-color: green;" text="Add">
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
                <effect>
                    <Glow />
                </effect>
                <font>
                    <Font name="System Bold" size="12.0" />
                </font></Button>
            <Button id="removeButton" layoutX="792.0" layoutY="579.0" mnemonicParsing="false" onAction="#btnRemoveItem" style="-fx-background-color: maroon;" text="Remove">
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
                <effect>
                    <Glow />
                </effect>
                <font>
                    <Font name="System Bold" size="12.0" />
                </font></Button>
            <Button id="editButton" fx:id="btnEdit" layoutX="866.0" layoutY="579.0" mnemonicParsing="false" onAction="#btnEditItem" text="Edit ">
                <cursor>
                    <Cursor fx:constant="HAND" />
                </cursor>
                <effect>
                    <Glow />
                </effect>
                <font>
                    <Font name="System Bold" size="12.0" />
                </font></Button>
            <TableView fx:id="table" layoutX="-2.0" layoutY="75.0" prefHeight="497.0" prefWidth="981.0" style="-fx-background-color: beige;">
                <columns>
                    <TableColumn fx:id="categoryCol" prefWidth="125.20000839233398" text="Category " />
                    <TableColumn fx:id="itemCol" prefWidth="523.2000274658203" text="Item" />
                    <TableColumn fx:id="quantityCol" prefWidth="113.0" text="Quantity" />
                    <TableColumn fx:id="priceCol" prefWidth="133.39996337890625" text="Price" />
               <TableColumn fx:id="minQuantityCol" prefWidth="86.5999755859375" text="Min Quantity" />
                </columns>
            </TableView>
         <Button layoutX="25.0" layoutY="579.0" mnemonicParsing="false" onAction="#addQuantity" style="-fx-background-color: green;" text="+">
            <font>
               <Font size="18.0" />
            </font>
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor>
         </Button>
         <Button layoutX="73.0" layoutY="579.0" mnemonicParsing="false" onAction="#removeQuantity" prefHeight="39.0" prefWidth="36.0" style="-fx-background-color: maroon;" text="-">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
         <Button layoutX="918.0" layoutY="579.0" mnemonicParsing="false" onAction="#loadSchedule" text="Schedule" />
        </children>
    </Pane>

</VBox>
